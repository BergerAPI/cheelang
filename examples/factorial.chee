external func printf(string text, ...): int

; Simple Factorial Function
func factorial(int n): int {
	if n > 1 {
		return n * factorial(n - 1)
	}

	return 1
}

func main(): int {
	; This was pretty easy, right?
  	var result = factorial(9)

  	; Just printing the result.
	printf("%i", result)

	return 0
}
